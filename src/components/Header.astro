---
import { Button, Link } from "@nextui-org/react";
import { Download } from '@geist-ui/react-icons';
import { Divider } from "@nextui-org/react";


const projectTitle = "Implementation And Formal Verification of " + 
				"a Parallel Algorithm Based on Minimax " +
				"Applied to a Game as a Case Study Using " +
				"Concurrent Processes Algebra and Mcrl2."

---

<div class="flex flex-row items-center ml-2">
  <h1 class="m-4 text-gray-400">{projectTitle}</h1>
  <Button as={Link} color="success" href="/download" variant="flat" className="" isIconOnly>
    <Download />
  </Button>
</div>
<Divider></Divider>


