---
import React from "react";
import { Card, CardBody } from "@nextui-org/react";
import Formulas from "./Formulas.astro";
const { text, formula } = Astro.props;

---
<div class="flex p-4 shadow-sm border-gray-200 border-1 rounded-md w-[90%]">
  <Formulas formula={formula}></Formulas>
  <h1 class="mx-2">:</h1>
  <h1 class="mx-4">{text}</h1>
</div>


